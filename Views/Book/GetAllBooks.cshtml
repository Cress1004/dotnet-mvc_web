@model IEnumerable<BookModel>;
@{
    ViewData["Title"] = "Get All Books";
}

<div class="text-center">
    <div class="container">
        <h1 class="display-4">All Books</h1>
        <div class="row">
            @foreach (var book in Model)
            {
                <div class="col-md-4">
                    <partial name="_bookThumbnail" model="@book"/>
                </div>
            }
        </div>
    </div>
</div>
